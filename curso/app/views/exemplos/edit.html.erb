<h1>Editing exemplo</h1>

<% form_for(@exemplo) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :nome %><br />
    <%= f.text_field :nome %>
  </p>
  <p>
    <%= f.label :codigo %><br />
    <%= f.text_area :codigo %>
  </p>
  <p>
    <%= f.label :visivel %><br />
    <%= f.check_box :visivel %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @exemplo %> |
<%= link_to 'Back', exemplos_path %>